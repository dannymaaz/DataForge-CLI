# Copy this file to .env and set your values

# Input Excel workbook (relative to project root or absolute path)
EXCEL_PATH=data/input/your_workbook.xlsx

# Optional output directory for generated CSV files.
# Leave empty to use: <excel_folder>/csv_exports_local
CSV_OUTPUT_DIR=

# Optional: export only selected sheets
# Example: SHEET_NAMES=pagos,clientes,ventas
SHEET_NAMES=

# Keywords to detect date columns
DATE_KEYWORDS=fecha,date

# CSV output options
CSV_ENCODING=utf-8
CSV_DELIMITER=,
